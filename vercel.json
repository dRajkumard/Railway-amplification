{
    "version":2,
    "builds":[
        {
            "src":"src/main.ts",
            "use":"@vercel/node"
        }
    ],
    "routes":[
        {
            "src":"/(.*)",
            "dest":"src/main.ts",
            "methods":[
                "GET",
                "POST",
                "PUT",
                "DELETE"
            ]
        }
    ],
   
        "functions": {
          "src/main.ts": {
            "memory": 3008,
            "maxDuration": 60
          }
          }

}
